{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development"
            },
            "console": "integratedTerminal"
        }
    ]
}
